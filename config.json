{
   "name": "Meta News",
   "version": "1.0.0",
   "identifier": "app.here.meta-news",
   "description": "A plugin to generate news plugin",
   "icon": "icon.png",
   "createdBy": "Lifesign",
   "website": "https://github.com/FriendsOfHere/meta-news",
   "appcast": "https://raw.githubusercontent.com/FriendsOfHere/meta-news/master/appcast.xml",
   "script": "index.js",
   "defaultCategory": "fav",
   "displayOn": ["mini"],
   "preferences": [
      {
          "title": "* Feed 地址(XML返回)",
          "type": "text",
          "key": "feedUrl",
          "default": ""
      },
      {
          "title": "* 插件名",
          "type": "text",
          "key": "pluginName",
          "default": ""
      },
      {
          "title": "* 插件标识(identifier)",
          "type": "text",
          "key": "pluginIdentifier",
          "default": "app.here.hello-world"
      },
      {
          "title": "插件描述",
          "type": "text",
          "key": "pluginDescription",
          "default": ""
      },
      {
          "title": "mini window 描述",
          "type": "text",
          "key": "miniDetail",
          "default": ""
      },
   ]
}
